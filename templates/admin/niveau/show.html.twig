{% extends 'base.html.twig' %}

{% block title %}Niveau{% endblock %}

{% block body %}
    <h1>Niveau</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ niveau.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ niveau.nom }}</td>
            </tr>
            <tr>
                <th>AsName</th>
                <td>{{ niveau.asName }}</td>
            </tr>
            <tr>
                <th>AsDateDebut</th>
                <td>{{ niveau.asDateDebut ? niveau.asDateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>AsDateFin</th>
                <td>{{ niveau.asDateFin ? niveau.asDateFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EtsNom</th>
                <td>{{ niveau.etsNom }}</td>
            </tr>
            <tr>
                <th>EtsAddresse</th>
                <td>{{ niveau.etsAddresse }}</td>
            </tr>
            <tr>
                <th>EtsContact</th>
                <td>{{ niveau.etsContact }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('niveau_index') }}">back to list</a>

    <a href="{{ path('niveau_edit', {'id': niveau.id}) }}">edit</a>

    {{ include('niveau/_delete_form.html.twig') }}
